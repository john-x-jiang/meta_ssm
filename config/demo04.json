{
    "exp_name": "demo",
    "exp_id": "04",
    
    "model": {
        "type": "LatentODE",
        "args": {
            "input_dim": 32,
            "latent_dim": 64,
            "num_filters": 4,
            "init_dim": 3,
            "obs_dim": 10,
            "ode_layer": 3,
            "rnn_layers": 1,
            "rnn_bidirection": true,
            "train_init": false,
            "sample": true
        }
    },

    "data":{
        "data_set": "BouncingBallDataLoader",
        "data_dir": "data/bouncing_ball",
        "data_name": "mixed_gravity_8",
        "num_workers": 1,
        "k_shot": 4
    },

    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 5e-4,
            "amsgrad": false,
            "betas": [0.9, 0.999]
        }
    },
    "lr_scheduler": 0,

    "loss": "dmm_loss",
    "metrics": [
        "mse"
    ],

    "batch_size": 50,
    "device": 0,
    "training": {
        "epochs": 20,
        "overfit_single_batch": false,

        "save_period": 50,
        "loss_type": "bce",
        "domain": false,

        "monitor": "min val_loss",
        "early_stop": 50,

        "min_annealing_factor": 0.0,
        "anneal_update": 100,

        "total_len": 20,
        "reverse": false,

        "lambda": 1,
        "r1": 1,
        "r2": 1,
        "r3": 1
    },
    "evaluating": {
        "total_len": 40,
        "reverse": false,
        "domain": false
    },
    
    "best_model": "m_best"
}
