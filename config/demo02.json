{
    "exp_name": "demo",
    "exp_id": "02",
    
    "model": {
        "type": "MetaDynamics_LatentODE",
        "args": {
            "input_dim": 32,
            "latent_dim": 64,
            "emission_dim": 2048,
            "total_dim": 60,
            "obs_dim": 3,
            "ode_layer": 3,
            "rnn_layers": 1,
            "rnn_bidirection": true,
            "train_init": false,
            "sample": true
        }
    },

    "data":{
        "data_set": "BouncingBallDataLoader",
        "data_dir": "data/bouncing_ball",
        "data_name": "mixed_gravity_8",
        "num_workers": 0,
        "k_shot": 3
    },

    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 1e-3,
            "weight_decay": 1e-5,
            "amsgrad": true,
            "betas": [0.9, 0.999]
        }
    },
    "lr_scheduler": 0,

    "loss": "meta_loss",
    "metrics": [
        "mse", "vpt"
    ],

    "batch_size": 100,
    "device": 0,
    "training": {
        "epochs": 30,
        "overfit_single_batch": false,

        "save_period": 50,
        "loss_type": "mse",

        "monitor": "min val_loss",
        "early_stop": 50,

        "min_annealing_factor": 0.0,
        "anneal_update": 1000,

        "total_len": 60,
        "reverse": false,

        "lambda": 10,
        "r1": 1,
        "r2": 1,
        "r3": 1
    },
    "evaluating": {
        "total_len": 60,
        "reverse": false
    },
    
    "best_model": "m_best"
}
